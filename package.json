{
  "name": "amankan",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "preview": "turbo run preview",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.3",
    "turbo": "^1.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.22.0"
}
